@model IEnumerable<ProdStore.Product>

@{
    ViewData["Title"] = "Index";
}
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

<h1>Search Result</h1>

@if (Model.Count() == 0)
{
    <p>Nothing</p>
}
else
{
    <ul>
        @foreach (var product in Model)
        {
            <li><a href="@Url.Action("Index","Product", new { id = product.Id })">@product.Name</a></li>
        }
    </ul>
}





